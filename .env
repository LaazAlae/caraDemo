# Google Vision API (Easy setup, great results)
GOOGLE_VISION_API_KEY=x

# AWS (Optional - for even better results)
AWS_ACCESS_KEY_ID=x
AWS_SECRET_ACCESS_KEY=x

# Application Settings
SECRET_KEY=your-super-secret-key-change-in-production
